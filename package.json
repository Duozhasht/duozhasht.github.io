{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "My personal config for angular2 ts + webpack",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --display-error-details",
    "tt": "rm -rf /Applications/MAMP/htdocs/* dist/* && webpack --config config/webpack.prod.js --progress --profile --bail && cp dist/* /Applications/MAMP/htdocs",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.2.0",
